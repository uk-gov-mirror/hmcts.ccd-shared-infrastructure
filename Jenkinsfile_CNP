#!groovy
//noinspection GroovyUnusedAssignment
@Library("Infrastructure@ethosldata") _

withInfraPipeline('ccd') {
    onMaster {
        enableSlackNotifications('#ccd-master-builds')
    }
    onDemo {
        enableSlackNotifications('#ccd-demo-builds')
    }
    onPR {
        enableSlackNotifications('#ccd-pr-builds')
    }
    onEthosldata {
        env.TF_STATE_STORAGE_TEMPLATE = 'statestore'
        env.TF_STATE_CONTAINER_TEMPLATE = 'statectr'
    }
}
